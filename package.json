{
	"name": "vscode-get-nix-path",
	"displayName": "vscode get nix path",
	"description": "get absolute or relative to workspace root nix style path",
	"version": "0.0.3",
	"publisher": "wernert",
	"author": "wernert",
	"engines": {
		"vscode": "^1.50.0"
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:getNixPath.showclipboard",
		"onCommand:getNixPath.copyabsolutepath",
		"onCommand:getNixPath.copyrelativepath"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "getNixPath.showclipboard",
				"title": "show clipboard",
				"category": "GetNixPath"
			},
			{
				"command": "getNixPath.copyabsolutepath",
				"title": "get absolute nix path",
				"category": "GetNixPath"
			},
			{
				"command": "getNixPath.copyrelativepath",
				"title": "get relative nix path",
				"category": "GetNixPath"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "getNixPath.copyrelativepath",
					"group": "6_copypath@50"
				},
				{
					"command": "getNixPath.copyabsolutepath",
					"group": "6_copypath@51"
				}
			],
			"explorer/context": [
				{
					"command": "getNixPath.copyrelativepath",
					"group": "6_copypath@50"
				},
				{
					"command": "getNixPath.copyabsolutepath",
					"group": "6_copypath@51"
				}
			],
			"editor/title/context": [
				{
					"command": "getNixPath.copyrelativepath",
					"group": "6_copypath@50"
				},
				{
					"command": "getNixPath.copyabsolutepath",
					"group": "6_copypath@51"
				}
			]
		}
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.13.25",
		"@types/vscode": "^1.50.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.9.7",
		"vscode-test": "^1.3.0"
	}
}
