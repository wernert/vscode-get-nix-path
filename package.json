{
	"name": "jaztest",
	"displayName": "get nix path",
	"description": "copy relative and absolute nix style /path",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:jaztest.helloWorld",
		"onCommand:jaztest.copyabsolutepath",
		"onCommand:jaztest.copyrelativepath"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "jaztest.helloWorld",
				"title": "jaz Hello World"
			},
			{
				"command": "jaztest.copyabsolutepath",
				"title": "get absolute path"
			},
			{
				"command": "jaztest.copyrelativepath",
				"title": "get relative path"
			}
		],
		"menus": {
      "explorer/context": [
        {
          "command": "jaztest.copyrelativepath",
          "group": "6_copypath@50"
        },
        {
          "command": "jaztest.copyabsolutepath",
          "group": "6_copypath@51"
        }
      ],
      "editor/title/context": [
        {
          "command": "jaztest.copyrelativepath",
          "group": "6_copypath@50"
        },
        {
          "command": "jaztest.copyabsolutepath",
          "group": "6_copypath@51"
        }
      ]
    }
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
